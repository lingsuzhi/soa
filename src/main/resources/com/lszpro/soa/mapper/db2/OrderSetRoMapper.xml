<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lszpro.soa.mapper.db2.OrderSetRoDao">
	<select id="selectList" resultType="com.lszpro.soa.model.bo.OrderSetBO"
		parameterType="java.util.Map">
		SELECT
		id,name,leftStr,title,orderfile,adds,createTime,remake
		from o2o_order_set
		<where>
			<if test="name!=null and name!=''">
				name=#{name}
			</if>
		</where>
		ORDER BY createTime DESC
	</select>
	<select id="selectById" resultType="com.lszpro.soa.model.bo.OrderSetBO"
		parameterType="java.lang.String">
		SELECT
			 id,name,leftStr,title,orderfile,adds,createTime,remake
		from o2o_order_set
		where id=#{id}
	</select>
		 	
</mapper>